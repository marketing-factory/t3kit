{
  "name": "t3kit/t3kit",
  "description": "Starter kit for TYPO3 CMS. Tools, extensions, configurations and templates.",
  "type": "project",
  "keywords": ["t3kit", "typo3", "cms", "starter"],
  "homepage": "http://www.t3kit.com",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "t3kit Community",
      "homepage": "https://github.com/t3kit"
    }
  ],
  "support": {
    "issues": "https://github.com/t3kit/t3kit/issues",
    "forum": "https://typo3.slack.com/messages/t3kit",
    "source": "https://github.com/t3kit/t3kit",
    "docs": "https://github.com/t3kit/t3kit/blob/master/README.md"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "git",
      "url": "https://github.com/t3kit/theme_t3kit"
    },
    {
      "type": "git",
      "url": "https://github.com/georgringer/news"
    },
    {
      "type": "git",
      "url": "https://github.com/TYPO3-Solr/ext-solr"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.2",
    "helhum/typo3-console": "^5.3",
    "typo3/minimal": "^9.3",
    "typo3/cms-about": "^9.3",
    "typo3/cms-adminpanel": "^9.3",
    "typo3/cms-belog": "^9.3",
    "typo3/cms-beuser": "^9.3",
    "typo3/cms-felogin": "^9.3",
    "typo3/cms-fluid-styled-content": "^9.3",
    "typo3/cms-form": "^9.3",
    "typo3/cms-impexp": "^9.3",
    "typo3/cms-info": "^9.3",
    "typo3/cms-redirects": "^9.3",
    "typo3/cms-reports": "^9.3",
    "typo3/cms-rte-ckeditor": "^9.3",
    "typo3/cms-setup": "^9.3",
    "typo3/cms-seo": "^9.3",
    "typo3/cms-sys-note": "^9.3",
    "typo3/cms-t3editor": "^9.3",
    "typo3/cms-tstemplate": "^9.3",
    "typo3/cms-viewpage": "^9.3",
    "typo3/cms-taskcenter": "^9.3",
    "typo3/cms-sys-action": "^9.3",
    "typo3/cms-rsaauth": "^9.3",
    "doctrine/lexer": "^1.0",
    "doctrine/common": "^2.9",
    "georgringer/news": "dev-master",
    "t3kit/theme-t3kit": "dev-9test"
  },
  "require-dev": {
    "t3kit/db": "~2.7.0",
    "behat/behat": "~3.3.0",
    "behat/mink-extension": "~2.2",
    "behat/mink-goutte-driver": "~1.2.1",
    "behat/mink-selenium2-driver": "~1.3.1",
    "guzzlehttp/guzzle": "~6.3.0"
  },
  "scripts":{
    "helper:setupdb": "scripts/setupdb.sh",
    "helper:postcomposer": "scripts/post-composer-install.sh",
    "typo3-cms-scripts": [
			"typo3cms install:fixfolderstructure",
			"typo3cms install:generatepackagestates"
		],
    "post-autoload-dump": [
      "@typo3-cms-scripts",
      "@helper:postcomposer"
    ]
  },
  "extra": {
    "typo3/cms": {
        "web-dir": "web"
    }
  }
}
